{
  "name": "bluebird",
  "version": "1.0.4",
  "description": "A server software for Minecraft: Bedrock edition",
  "main": "src/BlueBird.js",
  "scripts": {
    "test": "node start.js"
  },
  "dependencies": {
    "bbmc-binarystream": "git+https://github.com/BlueBirdMC/BinaryStream.git",
    "bbmc-raknet": "git+https://github.com/BlueBirdMC/RakNet.git"
  },
  "author": "BlueBirdMC Team",
  "license": "GNU-3.0",
  "devDependencies": {
    "time-stamp": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BlueBirdMC/BlueBird"
  },
  "keywords": [
    "mcpe",
    "mcpe server"
  ],
  "bugs": {
    "url": "https://github.com/BlueBirdMC/BlueBird/issues"
  },
  "bin": "start.js",
  "pkg": {
    "assests": [
      "src/entity/**",
      "src/utils/*****",
      "src/network/****",
      "src/network/handler/*",
      "src/network/mcpe/*",
      "src/network/mcpe/protocol/*******************",
      "src/network/mcpe/protocol/types/*******",
      "src/network/mcpe/protocol/resources/*",
      "src/player/**",
      "src/utils/*****"
    ],
    "targets": [
      "node16"
    ]
  },
  "homepage": "https://github.com/BlueBirdMC/BlueBird"
}
